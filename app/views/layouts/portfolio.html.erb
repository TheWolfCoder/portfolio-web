<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title><%= @page_title %></title>

<META name="keywords" content="<%= @seo_keywords%>" />

<%= favicon_link_tag 'favicons/favicon' %>

  <%= stylesheet_link_tag    'portfolios', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>


   <%= render 'shared/portfolio_nav' %>
   <%= render 'shared/portfolio_masthead' unless logged_in?(:site_admin) %>

        <%= yield %>
   <%= render 'shared/portfolio_footer' %>

  <%= alerts %>
  <%= copyright_generator %>
</body>
</html>
